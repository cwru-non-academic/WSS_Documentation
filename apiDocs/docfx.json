{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "*.md",
          "**/*.md"
        ],
        "exclude": [
          "_site/**",
          "api/**",
          "external/**"
        ]
      },
      {
        "files": [
          "api/**.yml",
          "api/**/index.md",
          "api/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "external/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "WSS Documentation Hub",
      "_enableSearch": true,
      "showExternalInherited": false
    },
    "template": [
      "default"
    ],
    "xref": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "markdownEngineName": "markdig"
  }
}
